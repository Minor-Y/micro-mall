n              const start = new Date()\n              start.setTime(start.getTime() - 3600 * 1000 * 24 * 90)\n              picker.$emit('pick', [start, end])\n            },\n          },\n        ],\n      },\n      //  整体数据\n      overallData: [],\n    }\n  },\n}\n</script>\n\n<style lang=\"less\" scoped>\n.el-card {\n  text-align: left;\n  margin-bottom: 10px;\n  box-shadow: none;\n  span {\n    margin-right: 6px;\n    font-size: 14px;\n    font-weight: bold;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module"}