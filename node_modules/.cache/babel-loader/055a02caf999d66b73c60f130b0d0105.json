{"ast":null,"code":"import api from '../../../api/index';\nexport default {\n  data() {\n    return {\n      activeName: 'first',\n      searchValue: '',\n      searchSelect: '1',\n      options: [{\n        value: '1',\n        label: '主管'\n      }, {\n        value: '2',\n        label: '客服'\n      }],\n      accountQuery: {\n        query: '',\n        pagenum: 1,\n        pagesize: 10,\n        value: '1'\n      },\n      accountDate: [],\n      total: 0\n    };\n  },\n\n  created() {\n    this.getAccountList();\n  },\n\n  methods: {\n    handleClick(e) {\n      console.log(e);\n    },\n\n    handleSizeChange(size) {\n      this.accountQuery.pagesize = size;\n    },\n\n    handleCurrentChange(num) {\n      this.accountQuery.pagenum = num;\n    },\n\n    // 获取账号列表\n    async getAccountList() {\n      const {\n        data: res\n      } = await this.$axios.get(`${api.localUrl}/accounts`, {\n        params: this.accountQuery\n      });\n      console.log(res);\n      if (res.status !== 200) return this.$message.error('获取账号列表失败，请刷新重试');\n      this.accountDate = res.data;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0DA;AACA;EACAA;IACA;MACAC,mBADA;MAEAC,eAFA;MAGAC,iBAHA;MAIAC,UACA;QACAC,UADA;QAEAC;MAFA,CADA,EAKA;QACAD,UADA;QAEAC;MAFA,CALA,CAJA;MAcAC;QACAC,SADA;QAEAC,UAFA;QAGAC,YAHA;QAIAL;MAJA,CAdA;MAoBAM,eApBA;MAqBAC;IArBA;EAwBA,CA1BA;;EA2BAC;IACA;EACA,CA7BA;;EA8BAC;IACAC;MACAC;IACA,CAHA;;IAIAC;MACA;IACA,CANA;;IAOAC;MACA;IACA,CATA;;IAUA;IACA;MACA;QAAAlB;MAAA;QACAmB;MADA;MAGAH;MACA;MACA;IACA;;EAlBA;AA9BA","names":["data","activeName","searchValue","searchSelect","options","value","label","accountQuery","query","pagenum","pagesize","accountDate","total","created","methods","handleClick","console","handleSizeChange","handleCurrentChange","params"],"sourceRoot":"src/views/set","sources":["Permissions.vue"],"sourcesContent":["<template>\n  <div>\n    <div class=\"tab\">\n      <el-tabs v-model=\"activeName\" @tab-click=\"handleClick\">\n        <el-tab-pane label=\"账号管理\" name=\"first\">\n          <el-card>\n            <div class=\"account-card\">\n              <!-- 左边 -->\n              <div>\n                <el-button type=\"primary\" size=\"mini\" class=\"margin-r-10\">添加账号</el-button>\n                <el-select v-model=\"accountQuery.value\" placeholder=\"请选择\" size=\"mini\">\n                  <el-option v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\">\n                  </el-option>\n                </el-select>\n              </div>\n              <!-- 右边 -->\n              <div class=\"right\">\n                <el-input placeholder=\"请输入搜索内容\" v-model=\"searchValue\" class=\"input-with-select\" size=\"mini\">\n                  <el-select v-model=\"searchSelect\" slot=\"prepend\" placeholder=\"请选择\" size=\"mini\">\n                    <el-option label=\"手机号\" value=\"1\"></el-option>\n                    <el-option label=\"用户名\" value=\"2\"></el-option>\n                  </el-select>\n                  <el-button slot=\"append\" icon=\"el-icon-search\" size=\"mini\"></el-button>\n                </el-input>\n              </div>\n            </div>\n            <!-- 表格内容区 -->\n            <div>\n              <el-table :data=\"accountDate\" style=\"width: 100%\">\n                <el-table-column prop=\"username\" label=\"用户名\" width=\"180\"></el-table-column>\n                <el-table-column prop=\"role\" label=\"角色\" width=\"180\"></el-table-column>\n                <el-table-column prop=\"mobile\" label=\"手机号\"></el-table-column>\n                <el-table-column label=\"性别\">\n                  <template slot-scope=\"scope\">\n                    <el-tag :type=\"scope.row.sex === '男' ? '' : 'danger'\">{{scope.row.sex}}</el-tag>\n                  </template>\n                </el-table-column>\n                <el-table-column prop=\"status\" label=\"状态\"></el-table-column>\n                <el-table-column prop=\"createtime\" label=\"创建时间\"></el-table-column>\n              </el-table>\n            </div>\n            <!-- 分页 -->\n            <div class=\"pagination\">\n              <el-pagination background @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\" \n            :current-page=\"accountQuery.pagenum\" :page-sizes=\"[10, 20, 30, 40, 60]\" :page-size=\"accountQuery.pagesize\" \n            layout=\"total, sizes, prev, pager, next, jumper\" :total=\"total\">\n            </el-pagination>\n            </div>\n          </el-card>\n        </el-tab-pane>\n        <el-tab-pane label=\"角色管理\" name=\"second\">配置管理</el-tab-pane>\n      </el-tabs>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport api from '../../../api/index'\nexport default {\n  data() {\n    return {\n      activeName: 'first',\n      searchValue: '',\n      searchSelect: '1',\n      options: [\n        {\n          value: '1',\n          label: '主管',\n        },\n        {\n          value: '2',\n          label: '客服',\n        },\n      ],\n      accountQuery: {\n        query: '',\n        pagenum: 1,\n        pagesize: 10,\n        value: '1',\n      },\n      accountDate: [],\n      total: 0,\n\n    }\n  },\n  created() {\n    this.getAccountList()\n  },\n  methods: {\n    handleClick(e) {\n      console.log(e)\n    },\n    handleSizeChange(size) {\n      this.accountQuery.pagesize = size\n    },\n    handleCurrentChange(num) {\n      this.accountQuery.pagenum = num\n    },\n    // 获取账号列表\n    async getAccountList() {\n      const { data: res } = await this.$axios.get(`${api.localUrl}/accounts`, {\n        params: this.accountQuery\n      })\n      console.log(res)\n      if(res.status !== 200) return this.$message.error('获取账号列表失败，请刷新重试')\n      this.accountDate = res.data\n    },\n  },\n}\n</script>\n\n<style lang=\"less\" scoped>\n.margin-r-10 {\n  margin-right: 10px;\n}\n.tab {\n  ::v-deep .el-tabs__item {\n    padding: 0;\n    width: 66px;\n    display: flex;\n    justify-content: center;\n  }\n  ::v-deep .el-card__body {\n  }\n  .right {\n    .el-select {\n      width: 90px;\n    }\n  }\n}\n.account-card {\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 18px;\n}\n.pagination {\n  margin-top: 16px;\n  text-align: left;\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}