{"ast":null,"code":"import api from '../../../api/index';\nexport default {\n  data() {\n    return {\n      activeName: 'first',\n      searchValue: '',\n      searchSelect: '1',\n      options: [{\n        value: '1',\n        label: '主管'\n      }, {\n        value: '2',\n        label: '客服'\n      }],\n      accountQuery: {\n        query: '',\n        pagenum: 1,\n        pagesize: 10,\n        value: '1'\n      },\n      accountDate: [],\n      total: 0,\n      drawer: false,\n      squareUrl: 'https://cube.elemecdn.com/9/c2/f0ee8a3c7c9638a54940382568c9dpng.png',\n      editData: [],\n      roleData: []\n    };\n  },\n\n  created() {\n    this.getAccountList();\n  },\n\n  methods: {\n    handleClick(e) {\n      console.log(e);\n    },\n\n    handleSizeChange(size) {\n      this.accountQuery.pagesize = size;\n    },\n\n    handleCurrentChange(num) {\n      this.accountQuery.pagenum = num;\n    },\n\n    // 获取账号列表\n    async getAccountList() {\n      const {\n        data: res\n      } = await this.$axios.get(`${api.localUrl}/accounts`, {\n        params: this.accountQuery\n      });\n      console.log(res);\n      if (res.status !== 200) return this.$message.error('获取账号列表失败，请刷新重试');\n      this.accountDate = res.data;\n    },\n\n    // 改变账号状态\n    async changeStatus(statusData) {\n      const res = await this.$axios.put(`${api.localUrl}/accounts/${statusData.userid}`, {\n        status: statusData.status\n      });\n      if (res.status !== 200) return this.$message.error('更改账号状态失败，请刷新重试');\n      console.log(res);\n    },\n\n    // 显示抽屉\n    async showAccountEdit(acc) {\n      this.drawer = true;\n      this.editData = acc;\n      console.log(acc);\n      const {\n        data: res\n      } = await this.$axios.get(`${api.localUrl}/role`);\n      if (res.status !== 200) return this.$message.error('获取角色列表失败，请刷新重试');\n      this.roleDate = res.data;\n    },\n\n    // 抽屉关闭前的动作\n    drawerClose() {}\n\n  }\n};","map":{"version":3,"mappings":"AA6HA;AACA;EACAA;IACA;MA